{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/node_modules/ts-node/dist/_bin.js",
      "args": ["test.ts"],
      "cwd": "${workspaceFolder}",
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnFirstSessionStart"
    }
  ],
  "compounds": []
}
