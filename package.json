{
  "name": "gbf-raidfinder",
  "version": "1.0.0",
  "description": "JavaScript library for tracking Granblue Fantasy raid tweets from Twitter",
  "main": "build/index.js",
  "scripts": {
    "codecov": "codecov",
    "install": "npm run build",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frizz925/gbf-raidfinder-js.git"
  },
  "keywords": [
    "gbf",
    "granblue",
    "fantasy",
    "raid",
    "tweet",
    "twitter"
  ],
  "author": "Izra Faturrahman <Frizz925@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Frizz925/gbf-raidfinder-js/issues"
  },
  "homepage": "https://github.com/Frizz925/gbf-raidfinder-js#readme",
  "dependencies": {
    "deep-extend": "^0.5.0",
    "gbf-raidfinder-parser": "github:frizz925/gbf-raidfinder-parser",
    "twitter": "github:frizz925/node-twitter"
  },
  "devDependencies": {
    "@types/deep-extend": "^0.4.31",
    "@types/dotenv": "^4.0.2",
    "@types/jest": "^22.1.0",
    "@types/node": "^9.3.0",
    "@types/request": "^2.0.13",
    "codecov": "^3.0.0",
    "dotenv": "^5.0.0",
    "jest": "^22.1.4",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/test/**/*.(spec|test).(ts|tsx|js)"
    ]
  }
}
